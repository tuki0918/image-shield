  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "husky": "^9.1.7",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
